{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "analytics": "26a1b0cc-fc71-4702-8ed5-8a2100006425"
  },
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "supermarket": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/supermarket",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": true,
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "node_modules/bootstrap-icons/font/bootstrap-icons.css",
              "src/styles.css"
            ],
            "scripts": [
              "src/assets/js/vendor/modernizr-3.6.0.min.js",
              "node_modules/jquery/dist/jquery.min.js",
              "src/assets/js/vendor/jquery-migrate-3.3.0.min.js",
              "node_modules/bootstrap/dist/js/bootstrap.bundle.min.js",
              "node_modules/bootstrap/dist/js/bootstrap.min.js",
              "src/assets/js/plugins/slick.js",
              "src/assets/js/plugins/jquery.syotimer.min.js",
              "src/assets/js/plugins/jquery.instagramfeed.min.js",
              "src/assets/js/plugins/jquery.nice-select.min.js",
              "src/assets/js/plugins/wow.js",
              "src/assets/js/plugins/jquery-ui-touch-punch.js",
              "src/assets/js/plugins/jquery-ui.js",
              "src/assets/js/plugins/magnific-popup.js",
              "src/assets/js/plugins/sticky-sidebar.js",
              "src/assets/js/plugins/easyzoom.js",
              "src/assets/js/plugins/scrollup.js",
              "src/assets/js/plugins/ajax-mail.js",
              "src/assets/js/main.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "supermarket:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "supermarket:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "supermarket:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "supermarket:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "supermarket:serve:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "supermarket"
}
